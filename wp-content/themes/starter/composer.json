{
  "name": "upstatement/timber-starter-theme",
  "description": "Starter theme to build a Timber theme",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "email": "jared@upstatement.com",
      "name": "jarednova"
    }  
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
	  "timber/timber": "^1.19",
    "tgmpa/tgm-plugin-activation": "^2.6",
    "hellonico/timber-dump-extension": "^1.0"
  },
  "autoload": {
    "psr-4": {
        "Gosselink\\": "inc/Gosselink/",
        "Gosselink\\Blocks\\": "blocks/",
        "Gossechild\\": "../starter_child/inc/Gossechild/",
        "Gossechild\\Blocks\\": "../starter_child/blocks/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.16|6.*"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
